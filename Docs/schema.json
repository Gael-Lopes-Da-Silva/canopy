{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "wallpaper": {
      "type": "object",
      "description": "The wallpaper options.",
      "properties": {
        "path": {
          "type": "string",
          "description": "Path to the wallpaper image. Can be absolute, relative, or empty to use default.\n\nDefault: ./Assets/Wallpapers/default.png"
        }
      },
      "required": ["path"],
      "additionalProperties": false
    }
  },
  "required": ["wallpaper"],
  "additionalProperties": false
}
